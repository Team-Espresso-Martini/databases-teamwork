
@using ComputersFactory.Models
@model IList<ComputersFactory.Models.MySqlReport>

@{
    ViewBag.Title = "Task 6";
}

<h2>Task 6 Completed</h2>
<h3>Number of Reports: @Model.Count</h3>
<ul class="list-group">
    @foreach (var item in Model)
    {
        <li class="list-group-item">Date: @item.Model, Total Amount: $@item.TotalSales, Total Quantity: @item.TotalQuantity</li>
    }
</ul>